{
  "name": "antigravity",
  "version": "0.1.0",
  "description": "ML Project - Antigravity",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/index.js",
    "build": "tsc",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "setup": "npm install && npm run build",
    "docker:build": "docker build -t antigravity:latest .",
    "docker:dev": "docker-compose up",
    "docker:dev:down": "docker-compose down",
    "docker:prod": "docker build -t antigravity:prod --target production . && docker run -it antigravity:prod",
    "precommit": "npm run lint && npm run type-check && npm test"
  },
  "keywords": [
    "ml",
    "machine-learning",
    "antigravity"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
